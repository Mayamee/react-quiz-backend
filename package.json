{
  "name": "react-quiz-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "dev": "webpack --watch --config webpack.config.js",
    "build:project": "yarn build:clear && yarn build && copyfiles package.json yarn.lock build",
    "build:docker": "yarn build:project && docker build -t react-quiz-api .",
    "docker:run": "docker run --rm --add-host mongo.internal:172.17.0.2 --name react-quiz-api -p 8080:8080 react-quiz-api",
    "docker:build-run": "yarn build:docker && yarn docker:run",
    "build:clear": "cf build",
    "build": "webpack --config webpack.config.js",
    "start": "node app/index.js",
    "start:prod": "node index.js",
    "prettify": "prettier --write ."
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/nodemailer": "^6.4.5",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "clear-folder": "^3.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon-webpack-plugin": "^4.8.1",
    "prettier": "^2.7.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
